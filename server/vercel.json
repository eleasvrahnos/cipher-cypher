{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ciphercypher-server-6ff695f2e615545b.vercel.app/:path*"
    }
  ]
  }